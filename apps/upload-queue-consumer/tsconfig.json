{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ES2022",
		"lib": ["ES2022"],
		"types": ["@cloudflare/workers-types"],
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"strict": true,
		"noEmit": true,
		"isolatedModules": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src/**/*"]
}
